  "files": [
    "outline/",
    "round/",
    "solid/",
    "sharp/",
    "twotone/",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:vue && npm run build:react",
    "build:vue": "node ./scripts/build.js vue",
    "build:react": "node ./scripts/build.js react",
    "build-outline": "rimraf ./optimized/outline ./optimized/outline && svgo --config=svgo.outline.js -f ./src/outline -o ./optimized/outline",
    "build-solid": "rimraf ./optimized/solid ./optimized/solid && svgo --config=svgo.solid.js -f ./src/solid -o ./optimized/solid"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@svgr/core": "^5.5.0",
    "@vue/compiler-dom": "^3.2.23",
    "camelcase": "^6.2.1",
    "rimraf": "^3.0.2",
    "svgo": "^2.8.0"
  }
}
